{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "arrowParens": 0,
    "consistent-return": "off",
    "import/no-dynamic-require": 0,
    "no-console": 0 ,
    "no-plusplus": 0,
    "no-restricted-syntax": 0
  }
}
